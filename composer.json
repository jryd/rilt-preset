{
    "name": "usrNotfound/rilt-preset",
    "authors": [
        {
            "name": "Kamal Chapagain",
            "email": "k_chapagain@msn.com"
        }
    ],
    "keywords": [
        "rilt",
        "laravel tailwind preset",
        "laravel frontend preset",
        "laravel react preset",
        "laravel inertiajs preset"
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ISeekplant\\Ui\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ISeekplant\\Ui\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
